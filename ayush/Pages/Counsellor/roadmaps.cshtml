@page
@model ayush.roadmapsModel
@{
    ViewData["Title"] = "sessions";
    Layout = "~/Pages/Shared/_CounsellorLayout.cshtml";
    <link rel="stylesheet" href="~/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="~/css/responsive.bootstrap4.min.css">
}


<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>Roadmaps</h1>
            </div>

        </div>
    </div>
</section>

<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-4">
                <div class="card card-default">
                    <div class="card-header">
                        <h3 class="card-title">
                            <i class="fas fa-user-graduate"></i>
                            Career Path 1
                        </h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <input type="hidden" id="userId" value="@Model.UserId" />
                        <div class="form-group">

                            <textarea asp-for="@Model.UserInformation.CareerPathOne1" class="form-control" rows="3" placeholder="Enter ..."></textarea>
                        </div>
                        <div class="form-group">

                            <textarea asp-for="@Model.UserInformation.CareerPathOne2" class="form-control" rows="3" placeholder="Enter ..."></textarea>
                        </div>
                        <div class="form-group">

                            <textarea asp-for="@Model.UserInformation.CareerPathOne3" class="form-control" rows="3" placeholder="Enter ..."></textarea>
                        </div>
                        <div class="form-group">

                            <textarea asp-for="@Model.UserInformation.CareerPathOne4" class="form-control" rows="3" placeholder="Enter ..."></textarea>
                        </div>
                        <div class="form-group">

                            <textarea asp-for="@Model.UserInformation.CareerPathOne5" class="form-control" rows="3" placeholder="Enter ..."></textarea>
                        </div>

                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->
            </div>
            <div class="col-md-4">
                <div class="card card-default">
                    <div class="card-header">
                        <h3 class="card-title">
                            <i class="fas fa-user-graduate"></i>
                            Career Path 2
                        </h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">

                        <div class="form-group">

                            <textarea asp-for="@Model.UserInformation.CareerPathTwo1" class="form-control" rows="3" placeholder="Enter ..."></textarea>
                        </div>
                        <div class="form-group">

                            <textarea asp-for="@Model.UserInformation.CareerPathTwo2" class="form-control" rows="3" placeholder="Enter ..."></textarea>
                        </div>
                        <div class="form-group">

                            <textarea asp-for="@Model.UserInformation.CareerPathTwo3" class="form-control" rows="3" placeholder="Enter ..."></textarea>
                        </div>
                        <div class="form-group">

                            <textarea asp-for="@Model.UserInformation.CareerPathTwo4" class="form-control" rows="3" placeholder="Enter ..."></textarea>
                        </div>
                        <div class="form-group">

                            <textarea asp-for="@Model.UserInformation.CareerPathTwo5" class="form-control" rows="3" placeholder="Enter ..."></textarea>
                        </div>

                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->
            </div>
            <div class="col-md-4">
                <div class="card card-default">
                    <div class="card-header">
                        <h3 class="card-title">
                            <i class="fas fa-user-graduate"></i>
                            Career Path 3
                        </h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">

                        <div class="form-group">

                            <textarea asp-for="@Model.UserInformation.CareerPathThree1" class="form-control" rows="3" placeholder="Enter ..."></textarea>
                        </div>
                        <div class="form-group">

                            <textarea asp-for="@Model.UserInformation.CareerPathThree2" class="form-control" rows="3" placeholder="Enter ..."></textarea>
                        </div>
                        <div class="form-group">

