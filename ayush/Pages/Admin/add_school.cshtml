@page
@model ayush.Pages.Admin.add_schoolModel
@{
    Layout = "~/Pages/Shared/_AdminLayout.cshtml";
}
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>Register School</h1>
            </div>

        </div>
    </div><!-- /.container-fluid -->
</section>

<!-- Main content -->
<section class="content">
    <div class="container-fluid">
        <form asp-route-returnUrl="@Model.ReturnUrl" method="post">
            <!-- SELECT2 EXAMPLE -->
            <div class="card card-default">

                <!-- /.card-header -->
                <div class="card-body">
                    <div class="row">


                        <div class="col-md-6">
                            <div class="form-group">
                                <label>School Name</label>
                                <input asp-for="asi.SchoolName" class="form-control" placeholder="School Name" />

                                <span asp-validation-for="asi.SchoolName" class="text-danger"></span>

                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="asi.Address"></label>
                                <input asp-for="asi.Address" class="form-control" placeholder="address" />

                                <span asp-validation-for="asi.Address" class="text-danger"></span>

                            </div>
                        </div>


                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="asi.PhoneNumber"></label>
                                <input asp-for="asi.PhoneNumber" class="form-control" placeholder="phnoenumber" />

                                <span asp-validation-for="asi.PhoneNumber" class="text-danger"></span>

                            </div>

                        </div>

                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="asi.Email"></label>
                                <input asp-for="asi.Email" class="form-control" placeholder="email" />

                                <span asp-validation-for="asi.Email" class="text-danger"></span>

                            </div>
                        </div>

                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="asi.Name_POC"></label>
                                <input asp-for="asi.Name_POC" class="form-control" placeholder="name(poc)" />

                                <span asp-validation-for="asi.Name_POC" class="text-danger"></span>

                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="asi.PhoneNumber_POC"></label>
                                <input asp-for="asi.PhoneNumber_POC" class="form-control" placeholder="phonen(poc)" />

                                <span asp-validation-for="asi.PhoneNumber_POC" class="text-danger"></span>

                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="asi.Email_POC"></label>
                                <input asp-for="asi.Email_POC" class="form-control" placeholder="Email" />

                                <span asp-validation-for="asi.Email_POC" class="text-danger"></span>

                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="asi.Address_POC"></label>
                                <input asp-for="asi.Address_POC" class="form-control" placeholder="address(poc)" />

                                <span asp-validation-for="asi.Address_POC" class="text-danger"></span>

                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="asi.HighQualification_POC"></label>
                                <input asp-for="asi.HighQualification_POC" class="form-control" placeholder="HQ" />

                                <span asp-validation-for="asi.HighQualification_POC" class="text-danger"></span>

                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="asi.Designation_POC"></label>
                                <input asp-for="asi.Designation_POC" class="form-control" placeholder="DE" />

                                <span asp-validation-for="asi.Designation_POC" class="text-danger"></span>

                            </div>
                        </div>



                        <div class="col-md-6">
                            <div class="form-group">
                                <label class="font-weight-light" for="exampleInputFile">Upload Certifications(POC)</label>
                                <div class="input-group">
                                    <div class="custom-file">
                                        <input type="file" class="custom-file-input" id="exampleInputFile">
                                        <label class="custom-file-label" for="exampleInputFile">Choose file</label>
                                    </div>
                                    <div class="input-group-append">
                                        <span class="input-group-text" id="">Upload</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label class="font-weight-light" for="exampleInputFile">Upload CV(POC)</label>
                                <div class="input-group">
                                    <div class="custom-file">
                                        <input type="file" class="custom-file-input" id="exampleInputFile">
                                        <label class="custom-file-label" for="exampleInputFile">Choose file</label>
                                    </div>
                                    <div class="input-group-append">
                                        <span class="input-group-text" id="">Upload</span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="asi.Password"></label>
                                <input asp-for="asi.Password" class="form-control" placeholder="Email" />

                                <span asp-validation-for="asi.Password" class="text-danger"></span>

                            </div>

                            <!-- /.close here -->
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="asi.ConfirmPassword"></label>
                                <input asp-for="asi.ConfirmPassword" class="form-control" placeholder="Email" />

                                <span asp-validation-for="asi.ConfirmPassword" class="text-danger"></span>

                            </div>
                        </div>
                        <!-- /.row -->
                        <!-- /.row -->
                    </div>
                    <!-- /.card-body -->
                    <div class="card-footer text-right">
                        <button type="submit" class="btn btn-primary">Register</button>
                    </div>


                </div>
            </div>
        </form>
    </div>
</section>
