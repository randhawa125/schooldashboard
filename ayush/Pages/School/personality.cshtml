    <script>
        var ctx = document.getElementById("myChart").getContext('2d');
        var myChart = new Chart(ctx, {
            type: 'pie',
            data: {
                labels: ["Experiential Mindset", "Extrovert", "Emotional", "Dependable", "Entrepreneurial Mindset", "Opportunistic"],
                datasets: [{
                    backgroundColor: [
                        "#2ecc71",
                        "#3498db",
                        "#95a5a6",
                        "#9b59b6",
                        "#f1c40f",
                        "#e74c3c"
                    ],
                    data: [12, 19, 3, 17, 28, 24]
